{
  "manifest_version": 2,
  "name": "JU GIFU Excel Maker",
  "short_name": "JU GIFU",
  "description": "It will structure your data to copy paste into excel table",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js"]
    }
  ],
  "browser_action": {
    "default_icon": "excel.png"
  },
  "permissions": [
    "activeTab"
  ]
}