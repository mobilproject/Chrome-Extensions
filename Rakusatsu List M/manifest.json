{
  "manifest_version": 2,
  "name": "Lot Number list maker",
  "short_name": "Lot Number list maker",
  "description": "Easily select cars by lot number and auction site",
  "version": "0.1",
  "background": {
    "scripts": ["jquery-3.2.1.slim.js", "background.js"]
	
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.slim.js"]
    }
  ],
  "browser_action": {
    "default_icon": "Car-List-512.png"
	 //"default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}