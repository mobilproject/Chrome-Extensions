{
  "manifest_version": 2,
  "name": "Identify Cars without prices",
  "short_name": "ID non priced cars",
  "description": "It will walk through a list vehicles to find cars without prices",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js"]
    }
  ],
  "browser_action": {
    "default_icon": "caricon.png"
  },
  "permissions": [
    "activeTab"
  ]
}