{
  "manifest_version": 2,
  "name": "Direct List Maker TC Web",
  "short_name": "Direct List Maker",
  "description": "Allows entering prices directly",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js"]
    }
  ],
  "browser_action": {
    "default_icon": "images/bidicon.png"
  },
  "permissions": [
    "activeTab", "https://www.iauc.co.jp/", "cookies"
  ]
}